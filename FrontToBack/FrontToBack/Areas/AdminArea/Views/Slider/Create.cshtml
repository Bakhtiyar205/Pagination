@model SliderVM
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Photos"></label>
            <input class="form-control" asp-for="Photos" style="width:350px;">
            <span class="text-danger" asp-validation-for="Photos"></span>
            <label asp-for="SubTitle"></label>
            <input type="text" class="form-control" asp-for="SubTitle" style="width:350px;">
            <span class="text-danger" asp-validation-for="SubTitle"></span>
        </div>

        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>